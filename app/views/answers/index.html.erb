<h1>Respostas</h1>

<table  class="table table-hover">
  <tr>
    <th>id</th>
    <th>morador_id</th>
    <th>pergunta_id</th>
    <th>texto</th>
    <th>apo_id</th>
    <th>comodo_id</th>
    <th>conceito_id</th>
    <th>atributo_id</th>
    <th>siscronizado</th>
  </tr>
 
<% @answers.each do |answer| %>
  <tr>
    <td><%= answer.id %></td>
    <td><%= answer.resident_id %></td>
    <td><%= answer.question_id %></td>
    <td><%= answer.text_ %></td>
    <td><%= answer.apo_id %></td>
    <td><%= answer.room_id %></td>
    <td><%= answer.concept_id %></td>
    <td><%= answer.attribute_id %></td>
    <td><%= answer.synchronized %></td>
  </tr>
<% end %>
</table>